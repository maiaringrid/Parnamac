$TTL    604800
@       IN      SOA     ns.parnaflix.com.br. root.com.br. (
                        2         ; Serial
                        604800    ; Refresh
                        86400     ; Retry
                        2419200   ; Expire
                        604800 )  ; Negative Cache TTL

; Registros de DNS
@          IN  NS     ns.parnaflix.com.br.       ; Define o servidor de nomes (NS) da zona como ns.parnamacflix.com.br
@          IN  A      192.168.0.6               ; Define o IP do domínio principal (parnamacflix.com.br)

ns         IN  A      192.168.0.6               ; Define o IP para o host ns.parnamac.com.br (servidor de DNS)
hotsite    IN  A      192.168.0.6               ; Define o IP para o host webmail.parnamac.com.br (servidor de e-mail)
portal     IN  A      192.168.0.6               ; Define o IP para o host portal.parnamac.com.br (Portal do provedor)
sign     IN  A      192.168.0.6 
backend  IN  A      192.168.0.6 